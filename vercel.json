{
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/anuraghazra/github-readme-stats"
    },
    {
      "source": "/git",
      "destination": "https://github.com/MadeByThePinsHub/gh-readme-stats-vercel"
    },
    {
      "source": "/sources",
      "destination": "https://github.com/MadeByThePinsHub/gh-readme-stats-vercel"
    },
    {
      "source": "/src",
      "destination": "https://github.com/MadeByThePinsHub/gh-readme-stats-vercel"
    }
  ]
}
